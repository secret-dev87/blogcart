<AuthorizeView>
    <Authorized>
        <MudAppBar Elevation="21" Dense="true">
            <MudIconButton Icon="@Icons.Material.Filled.Menu" Color="Color.Primary" Edge="Edge.Start"
                           OnClick="async () => await OnSidebarToggled.InvokeAsync()" />
            <MudSpacer />
            <MudButton Href="Identity/Account/Logout" StartIcon="@Icons.Filled.Logout" Variant="Variant.Filled" Size="Size.Small" Color="Color.Inherit">Log-out</MudButton>
        </MudAppBar>
    </Authorized>
    <NotAuthorized>
        <MudAppBar Elevation="21" Dense="true">
            <MudIconButton Icon="@Icons.Material.Filled.Menu" Color="Color.Primary" Edge="Edge.Start"
                           OnClick="async () => await OnSidebarToggled.InvokeAsync()" />
            <MudButton Href="/home" StartIcon="@Icons.Filled.Home" Variant="Variant.Filled" Size="Size.Small" Color="Color.Inherit"></MudButton>
            <MudSpacer />
            @*<MudButton Href="Identity/Account/login" StartIcon="@Icons.Filled.Logout" Variant="Variant.Filled" Size="Size.Small" Color="Color.Inherit">Please Register or LogIn</MudButton>*@
        </MudAppBar>
    </NotAuthorized>
</AuthorizeView>


@code {
    [Parameter]
    public EventCallback OnSidebarToggled { get; set; }
}